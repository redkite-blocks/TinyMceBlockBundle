{
    "name": "redkite-cms/tinymce-block-bundle",
    "description": "Adds the TinyMCE editor to RedKite CMS to manage an hipertext content on the webpage",
    "keywords": ["redkite", "cms", "app"],
    "homepage": "http://redkite-labs.com",
    "type": "redkite-cms-app",
    "license": "MIT",
    "authors": [
        {
          "name": "RedKite Labs",
          "email": "info@redkite-labs.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://apps.redkite-labs.com/"
        }
    ],
    "require-dev": {
        "redkite-labs/redkite-cms-bundle": "dev-master",
        "willdurand/propel-typehintable-behavior": "dev-master"
    },
    "autoload": {
        "psr-0": { "RedKiteCms\\Block\\TinyMceBlockBundle": ""
        }
    },
    "target-dir" : "RedKiteCms/Block/TinyMceBlockBundle",
    "minimum-stability": "dev"
}
